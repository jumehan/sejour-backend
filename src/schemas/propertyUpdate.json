{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://sejour.com/propertyNew.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 25
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "integer",
      "minimum": 0
    }
  },
  "additionalProperties": false,
  "required": ["title", "description", "price"]
}

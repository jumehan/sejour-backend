{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://sejour.com/propertyNew.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 25
    },
    "street": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "city": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "state": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "zipcode": {
      "type": "string",
      "minLength": 1,
      "maxLength": 10
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "integer",
      "minimum": 0
    }
  },
  "additionalProperties": false,
  "required": [
    "title",
    "street",
    "city",
    "state",
    "zipcode",
    "description",
    "price"
  ]
}